# Taken from (https://github.com/garybernhardt/dotfiles)

[user]
    name = 
    email = 
[alias]
    st = status
    ci = commit --verbose
    co = checkout
    di = diff --find-copies-harder
    dc = diff --find-copier-harder --cached
    amend = commit --amend
    aa = add --all
    ff = merge --ff-only
    noff = merge --no-ff
    fa = fetch --all
    pom = push origin main
    ds = diff --stat=160,120
    dh1 = diff HEAD~1
    
    # Divergence (commits we added and commits remote added)
    div = divergence
